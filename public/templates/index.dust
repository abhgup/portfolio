    <script>
    $(document).ready(function(){
        
        function noop(e) {
          e.stopPropagation();
          e.preventDefault();
        }

        $('#submitForm').on('click', function(e){
            noop(e);
            var csrf = $('#csrf').val(),
                name = $('#name').val(),
                email = $('#email').val(),
                phone = $('#phone').val(),
                message = $('#message').val();

            var formData = new FormData();
            formData.append('_csrf', csrf);
            formData.append('name', name);
            formData.append('email', email);
            formData.append('phone', phone);
            formData.append('message', message);

            $.ajax({
                url: '/sendEmail',
                type: 'POST',
                data: formData,
                processData: false, 
                contentType: false, 
            }).done(function (result) {
                if (result) {
                  alert("Your mail has been successsfully delivered");
                } else {
                  alert("We are having trouble processing you reauest. Please try again later");
                }
            });

        });
    });
    </script>

